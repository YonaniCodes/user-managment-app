datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String
  email     String   @unique
  password  String
  role      String
  joinedAt  DateTime @default(now())
  lastLogin DateTime
  status    String
  createdAt DateTime @default(now())
}
